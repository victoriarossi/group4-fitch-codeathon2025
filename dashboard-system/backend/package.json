{
  "name": "fitch-sustainability-backend",
  "version": "1.0.0",
  "description": "Backend API for FitchGroup Sustainability Dashboard",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "migrate": "ts-node src/database/migrations/run-migrations.ts",
    "seed": "ts-node src/database/seeds/run-seeds.ts"
  },
  "keywords": [
    "sustainability",
    "emissions",
    "esg"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.10.9",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  }
}
